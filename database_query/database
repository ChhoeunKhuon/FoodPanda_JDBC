Food_Panda JDBC table - Basic:create table customer(    id           integer generated by default as identity        constraint customer_pk            primary key,    name         varchar,    email        varchar,    password     varchar,    is_deleted   boolean,    created_date date);alter table customer    owner to postgres;create table product(    id                  integer generated by default as identity        constraint product_pk            primary key,    product_name        varchar,    product_code        varchar,    is_deleted          boolean,    imported_at         date,    expired_at          date,    product_description varchar);alter table product    owner to postgres;create table "order"(    id                integer generated by default as identity        constraint order_pk            primary key,    order_name        varchar,    order_description integer,    cus_id            integer        constraint order_customer_id_fk            references customer,    ordered_at        date);alter table "order"    owner to postgres;create table product_order(    pro_id   integer        constraint product_order_product_id_fk            references product,    order_id integer        constraint product_order_order_id_fk            references "order");alter table product_order    owner to postgres;